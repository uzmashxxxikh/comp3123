function angle_Type(angle) {
    if (angle > 0 && angle < 90) {
        return "Acute Angle";
    } else if (angle === 90) {
        return "Right Angle";
    } else if (angle > 90 && angle < 180) {
        return "Obtuse Angle";
    } else if (angle === 180) {
        return "Straight Angle";
    } else {
        return "Invalid Angle";
    }
}

console.log(angle_Type(47));   
console.log(angle_Type(90));   
console.log(angle_Type(145));  
console.log(angle_Type(180));  

